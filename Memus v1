local offsets = {
    {1, 2},
    {0.7, 1.7},
    {0.5, 1.5},
    {0.2, 1.2},
    {0, 1},
    {-0.2, 0.7},
    {-0.5, 0.5},
    {-0.7, 0.2}
}
local num = 0
function cleave()
if Active == true then return end
    Active = true
    if mouse.Target and mouse.Target.Parent:FindFirstChild("Humanoid") then
        local targethrp = mouse.Target.Parent:FindFirstChild("HumanoidRootPart")
	    local mag = (targethrp.Position - hrp.Position).magnitude
	    local mousetarget = mouse.Target.Parent
        rs.Anchor:FireServer(mousetarget.HumanoidRootPart, true)    
        donutanimation = Instance.new("Animation")
        donutanimation.AnimationId = assettest(5426634253)
        donutanim = plr.Character.Humanoid:LoadAnimation(donutanimation)
        donutanim:Play()
        donutanim:AdjustSpeed(1)
        wait(0.35)
        rs.Damage11:FireServer(hum, hrp.CFrame * CFrame.new(1, 2, -0.5), 1, 0, Vector3.new(0, 0, 0), 0.004, "rbxassetid://429400881", 1, math.huge)
        for i = 1,8 do
        for _, offset in ipairs(offsets) do
        rs.Damage12:FireServer(hum, mousetarget.HumanoidRootPart.CFrame * CFrame.new(offset[1], offset[2], -0.5), 0, 0, Vector3.new(0, 0, 0), 0.004, "", 1, 1)
        end
        wait(0.1)
        end
        wait()
        donutanim:AdjustSpeed(1)
        rs.Damage11Sans:FireServer(plr.Character.Humanoid, mousetarget.Head.CFrame, 99, 0, Vector3.new(0, 0, 0), 9e99, "rbxassetid://438666196", 1, 20)  
        rs.Damage11Sans:FireServer(plr.Character.Humanoid, mousetarget.Head.CFrame, 99, 0, Vector3.new(0, 0, 0), 9e99, "rbxassetid://438666196", 1, 20)  
        rs.Damage11Sans:FireServer(plr.Character.Humanoid, mousetarget.Head.CFrame, 99, 0, Vector3.new(0, 0, 0), 9e99, "rbxassetid://438666196", 1, 20)  
        rs.Damage11Sans:FireServer(plr.Character.Humanoid, mousetarget.Head.CFrame, 99, 0, Vector3.new(0, 0, 0), 9e99, "rbxassetid://438666196", 1, 20)  
        repeat
	        num = num + 1
	    	wait(0.1)
	    until num == 2
        rs.Anchor:FireServer(mousetarget.HumanoidRootPart, false)
        for i = 1,9 do   
            rs.CalculatedStrike:FireServer(plr.Character.Humanoid, mousetarget.Head.CFrame, 0.0001, 0, Vector3.new(0, 0, 0), 99999999999999, "rbxassetid://241837157", 1, 0)          
        end
        rs.HamonSword:FireServer(mousetarget.Humanoid, 350, 0, Vector3.new(0, 50, 0))              
        rs.HamonSword:FireServer(mousetarget.Humanoid, 350, 0, Vector3.new(0, 50, 0))              
        for i = 1,2 do
        local player = game.Players.LocalPlayer
        local shakeIntensity = 1
        shakeCamera(player, shakeIntensity)
        end      
        num = 0
    end
    wait(0.2)
    wait(0.2)
    Active = false
end


