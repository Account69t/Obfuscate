function Red()
    if act == true then return end
    act = true
    kickanimation = Instance.new("Animation")
    kickanimation.AnimationId = assettest(4459137517)
    kickanim = plr.Character.Humanoid:LoadAnimation(kickanimation)
    kickanim:Play()
    kickanim:AdjustSpeed(1)
    wait(0.3)
    kickanim:AdjustSpeed(0)
    rs.Damage11Sans:FireServer(hrp.Parent.Humanoid, hrp.Parent.Head.CFrame, 0.0001, 0, Vector3.new(0, 0, 0), 9e99, "rbxassetid://182765513", 1, math.huge)
    for i = 1,20 do
    rs.Damage12:FireServer(hum, la.CFrame * CFrame.new(0, -0.5, 0), 0, 0, Vector3.new(0, 0, 0), 0.002, "", 1, 1)
    rs.Damage12:FireServer(hum, la.CFrame * CFrame.new(math.random(-1, 1), math.random(-1, -0.5), math.random(-1, 1)), 0, 0, Vector3.new(0, 0, 0), 0.001, "", 1, 1)
    wait(0.05)
    end
    rs.Damage11Sans:FireServer(hrp.Parent.Humanoid, hrp.Parent.Head.CFrame, 0.0001, 0, Vector3.new(0, 0, 0), 9e99, "rbxassetid://187137543", 1, math.huge)
    local Design = 0.002
    rs.Damage12:FireServer(hum, la.CFrame * CFrame.new(0, -0.5, 0), 0, 0, Vector3.new(0, 0, 0), 0.27, "", 1, 1)
    rs.Damage12:FireServer(hum, la.CFrame * CFrame.new(0, -0.5, 0), 0, 0, Vector3.new(0, 0, 0), 0.27, "", 1, 1)
    rs.Damage12:FireServer(hum, la.CFrame * CFrame.new(0, -0.5, 0), 0, 0, Vector3.new(0, 0, 0), 0.27, "", 1, 1)
    wait()
    kickanim:AdjustSpeed(1)
    for i = 1,20 do
    rs.Damage12:FireServer(hum, plr.Character.HumanoidRootPart.CFrame * CFrame.new(math.random(-20, 20), math.random(0, 20), math.random(-20, 20)), 0, 0, Vector3.new(0, 0, 0), 0.02, "", 1, 1)
    rs.Damage12:FireServer(hum, plr.Character.HumanoidRootPart.CFrame * CFrame.new(math.random(-30, 20), math.random(0, 30), math.random(-30, 30)), 0, 0, Vector3.new(0, 0, 0), 0.01, "", 1, 1)
    end
    for i,v in pairs(game.Workspace.Entities:GetChildren()) do
        if v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Name ~= game.Players.LocalPlayer.Name then
            if (hrp.Position - v.HumanoidRootPart.Position).Magnitude < 20 then
                rs.Knock:FireServer(v.Humanoid)
                rs.Damage:FireServer(v.Humanoid, v.HumanoidRootPart.CFrame, 100, 0.25, (hrp.Position - v.Humanoid.RootPart.Position).Unit * -65 + Vector3.new(0, 60, 0), "rbxassetid://241837157", 0, Color3.new(100, 100, 255), "rbxassetid://12222058", 0.4, 0)
            end
        end
    end
    for i = 1,2 do
    local player = game.Players.LocalPlayer
    local shakeIntensity = 1.5
    shakeCamera(player, shakeIntensity)
    end
    act = false
end

---------------------------------Domain--------------------------------------------

function Do0()
    local center = hrp.Position  
    local radius = 60  
    local numPoints = 50
    
    for i = 1, numPoints do
        local angle = math.pi * 5 * (i / numPoints)
        local x = math.cos(angle) * radius
        local z = math.sin(angle) * radius
        local position = center + Vector3.new(x, 0, z)
        rs.VampireRush:FireServer(hum, CFrame.new(position), 1e-34, 0, Vector3.new(),"rbxassetid://3370684035", 0.5, Color3.new(10, 10, 105), "rbxassetid://", 1, 0.5)
    end
end
