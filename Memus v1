        end
    end
end
end))
function godmode()
    rs.Block:FireServer(true)
    game.ReplicatedStorage.StandsSlam:FireServer(game.Players.LocalPlayer.Character.Humanoid, -0 * math.huge)
end
godmode()
function assettest(Id)
    return "rbxassetid://"..Id
