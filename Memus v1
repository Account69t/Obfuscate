punch1 = true
function punch()
if Active == true then return end
if punches == 1 then
    Active = true
    m1()
    punches = 2
    wait(0.2)
    Active = false
elseif punches == 2 then
    Active = true
    m2()
    punches = 3
    wait()
    Active = false
else
    Active = true
    m3()
    punches = 1
    wait(0.5) 
    Active = false
    end
end

mouse.Button1Down:connect(function()
if death == true then return end
	punch()
end)


-------------------------Lapse Blue---------------------
local blue = 0
function lapse()
    if act == true then return end
    act = true
    punch1animation = Instance.new("Animation")
    punch1animation.AnimationId = assettest(5040043856)
    nhuc1anim = plr.Character.Humanoid:LoadAnimation(punch1animation)
    nhuc1anim:Play()
    nhuc1anim:AdjustSpeed(0.4)
    wait(0.9)
    nhuc1anim:AdjustSpeed(0)
    wait(2)
    local hitbox1 = Instance.new("Part")
    hitbox1.CFrame = CFrame.new(mouse.Hit.p)
    hitbox1.Name = "hitbox"
    hitbox1.Anchored = true
    hitbox1.CanCollide = false
    hitbox1.Size = Vector3.new(1, 1, 1)
    hitbox1.Massless = true
    hitbox1.Transparency = 0
    hitbox1.Color = Color3.new(0,0,0)
    hitbox1.Parent = game.Players.LocalPlayer.Character
    rs.Damage11Sans:FireServer(hrp.Parent.Humanoid, hrp.Parent.Head.CFrame, 0.0001, 0, Vector3.new(0, 0, 0), 9e99, "rbxassetid://1146698075", 1, math.huge)
    blue = blue + 0.009
    rs.Damage12Sans:FireServer(hum, hitbox1.CFrame, 0, 0, Vector3.new(0, 0, 0), blue, "", 1, 50)
    wait(0.3)
    local player = game.Players.LocalPlayer
    local shakeIntensity = 1.5
    shakeCamera(player, shakeIntensity)
    MainSoundSystem:Execute({["Action"] = "Create", ["Id"] = 1146698075,["Volume"] = math.huge,["Loop"] = false, ["StartPosition"] = 0})
    for i = 1, 50 do
    blue = blue + 0.008
    rs.Damage12Sans:FireServer(hum, hitbox1.CFrame, 0, 0, Vector3.new(0, 0, 0), blue, "", 1, 50)
    wait()
    end
    for i = 1, 350 do
    for i,v in pairs(game.Workspace.Entities:GetChildren()) do
        if v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Name ~= game.Players.LocalPlayer.Name then
            if (hitbox1.Position - v.HumanoidRootPart.Position).Magnitude < 100 then
            rs.Damage:FireServer(v.Humanoid, v.HumanoidRootPart.CFrame, 0.1, 0.25, (hitbox1.Position - v.Humanoid.RootPart.Position).Unit * 30, "rbxassetid://241837157", 0, Color3.new(100, 100, 255), "rbxassetid://12222058", 0.4, 0)
            end
        end
    end
    for i,v in pairs(game.Workspace.Entities:GetChildren()) do
        if v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Name ~= game.Players.LocalPlayer.Name then
            if (hitbox1.Position - v.HumanoidRootPart.Position).Magnitude < 30 then
            rs.Damage:FireServer(v.Humanoid, v.HumanoidRootPart.CFrame, 190, 0.25, (hitbox1.Position - v.Humanoid.RootPart.Position).Unit * 10, "rbxassetid://241837157", 0, Color3.new(100, 100, 255), "rbxassetid://12222058", 0.4, 0)
            end
        end
    end
    rs.Damage12Sans:FireServer(hum, hitbox1.CFrame, 0, 0, Vector3.new(0, 0, 0), 0.24, "", 1, 50)
    rser.Heartbeat:wait()
    end
    wait(0.4)
    blue = 0
    nhuc1anim:Stop()
    wait(0.5)
    act = false
end

