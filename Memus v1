	hitbox1.Massless = true
	hitbox1.Transparency = 0.7
	hitbox1.Color = Color3.new(0,0,0)
	hitbox1.Touched:Connect(function(hit)
    end)
end
local OldPos = nil
local HumName = nil
local CDC = false
function Expansion()
if CDC == true then return end
CDC = true
doanimation = Instance.new("Animation")
doanimation.AnimationId = assettest(4459137517)
doanim = plr.Character.Humanoid:LoadAnimation(doanimation)
doanim:Play()
doanim:AdjustSpeed(0.27)
OldPos = hrp.CFrame
rs.Damage11Sans:FireServer(hum, chr.Head.CFrame, 0, 0, Vector3.new(0, 0, 0), 9e99, "rbxassetid://7083916310", 1, math.huge)  
wait(0.1)
MainSoundSystem:Execute({["Action"] = "Create", ["Id"] = 7083916310, ["Volume"] = math.huge})
wait(1.85)  
doanim:AdjustSpeed(0)
for i = 1,53 do
Do0()
wait(0.1)
end
doanim:Stop()
for i,v in pairs(game.Workspace.Entities:GetChildren()) do
        if v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Name ~= game.Players.LocalPlayer.Name then
            if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).Magnitude < 60 then
                rs:WaitForChild("StarPlatinumStuff"):WaitForChild("SP3"):FireServer("Hit1",{game:GetService("Players").LocalPlayer.Character.Humanoid,Vector3.new(0,0,0),CFrame.new(-75017 + math.random(-30,30), 1522, 2346 + math.random(-30,30)),Vector3.new(0,0,0),0,0,0,v.Torso,nil,2,5},{game:GetService("Workspace").MIHBass},{5})
                task.wait()
                rs.Anchor:FireServer(v.HumanoidRootPart, true)
                HumName = v.Name
            end
        end
    end
    task.wait()
    hrp.CFrame = CFrame.new(-75017, 1522, 2346)
    Slime()
    for i = 1,40 do
        rs.Damage11Sans:FireServer(hum, hrp.CFrame, 0, 0, Vector3.new(0, 0, 0), 999999999999999, "", 0, 0)
        rs.Damage11:FireServer(hum, hrp.CFrame, 0, 0, Vector3.new(0, 0, 0), 999999999999999, "", 0, 0)
        rs.Damage11:FireServer(hum, hrp.CFrame, 0, 0, Vector3.new(0, 0, 0), 999999999999999, "", 0, 0)
        wait()
    end
    wait(5)
    hrp.CFrame = OldPos
    wait(4)
    CDC = false
end


