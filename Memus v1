            if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).Magnitude < 60 then
                rs.Damage:FireServer(v.Humanoid, v.Head.CFrame, 190, 0, Vector3.new(0, 0, 0), "rbxassetid://3370684035", 0, Color3.new(255, 0, 0), "rbxassetid://429400881", 1, 1)
            end
        end
    end
end
 
punch1 = true
function punch()
if Active == true then return end
if punches == 1 then
    Active = true
    m1()
    punches = 2
    wait(0.1)
    Active = false
elseif punches == 2 then
    Active = true
    m2()
    punches = 3
    wait(0.03)
    Active = false
else
    Active = true
    m3()
    punches = 1
    wait(0.5) 
    Active = false
    end
end

mouse.Button1Down:connect(function()
if death == true then return end
	punch()
end)



--------------------------------------Domain Expansion-------------------------------------------------

function Do0()
    local center = hrp.Position  
    local radius = 60  
    local numPoints = 50
    
    for i = 1, numPoints do
        local angle = math.pi * 5 * (i / numPoints)
        local x = math.cos(angle) * radius
        local z = math.sin(angle) * radius
        local position = center + Vector3.new(x, 0, z)
        
        rs.Damage12:FireServer(hum, CFrame.new(position), 0, 0, Vector3.new(0, 0, 0), 0.01, "", 1, 1)
    end
end

function Do1()
local offsets = {
  {x = 6, y = -2}, {x = 6, y = 0}, {x = 6, y = 2}, {x = 6, y = 4}, {x = 6, y = 6}, {x = 6, y = 8},
  {x = -6, y = -2}, {x = -6, y = 0}, {x = -6, y = 2}, {x = -6, y = 4}, {x = -6, y = 6}, {x = -9, y = 8},
  {x = -7, y = 8}, {x = -5, y = 8}, {x = -3, y = 8}, {x = -1, y = 8}, {x = 1, y = 8}, {x = 5, y = 8},
  {x = 3, y = 8}, {x = 7, y = 8}, {x = 9, y = 8}, {x = 7, y = 10}, {x = -7, y = 10}, {x = -12, y = 11.5},
  {x = -10, y = 11.5}, {x = -8, y = 11.5}, {x = -6, y = 11.5}, {x = -4, y = 11.5}, {x = -2, y = 11.5},
  {x = 0, y = 11.5}, {x = 2, y = 11.5}, {x = 4, y = 11.5}, {x = 6, y = 11.5}, {x = 8, y = 11.5},
  {x = 10, y = 11.5}, {x = 12, y = 11.5},
}



for _, offset in ipairs(offsets) do
    rs.Damage12:FireServer(hum, hrp.CFrame * CFrame.new(offset.x, offset.y, 8), 0, 0, Vector3.new(0, 0, 0), 0.02, "", 1, 1)
end
end


local CDC = false
function Expansion()
if CDC == true then return end
CDC = true
doanimation = Instance.new("Animation")
doanimation.AnimationId = assettest(3445824199)
doanim = plr.Character.Humanoid:LoadAnimation(doanimation)
doanim:Play()
doanim:AdjustSpeed(5)
rs.Damage11Sans:FireServer(hum, chr.Head.CFrame, 0, 0, Vector3.new(0, 0, 0), 9e99, "rbxassetid://6590147536", 1, math.huge)  
wait(0.1)
MainSoundSystem:Execute({["Action"] = "Create", ["Id"] = 6590147536, ["Volume"] = math.huge})
wait(2)  
Do0()
rs.Damage11Sans:FireServer(hum, chr.Head.CFrame, 0, 0, Vector3.new(0, 0, 0), 9e99, "rbxassetid://7817336081", 1, math.huge)
wait(1)
rs.Damage11Sans:FireServer(hum, chr.Head.CFrame, 0, 0, Vector3.new(0, 0, 0), 9e99, "rbxassetid://7817367014", 1, math.huge)
wait(0.3)  
MainSoundSystem:Execute({["Action"] = "Create", ["Id"] = 7817367014, ["Volume"] = math.huge})
wait(2)
Do1()
wait(0.5)
doanim:Stop()
rs.Damage:FireServer(game.Players.LocalPlayer.Character.Humanoid, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame, 10, 0, Vector3.new(),"rbxassetid://3370684035", 3.3, Color3.new(255, 0, 0), "rbxassetid://", 1, 0.5)
for i,v in pairs(game.Workspace.Entities:GetChildren()) do
        if v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Name ~= game.Players.LocalPlayer.Name then
            if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).Magnitude < 60 then
                rs.Damage:FireServer(v.Humanoid, v.Head.CFrame, 190, 0, Vector3.new(0, 0, 0), "rbxassetid://3370684035", 0, Color3.new(255, 0, 0), "rbxassetid://429400881", 1, 2)
                rs.Damage:FireServer(v.Humanoid, v.Head.CFrame, 190, 0, Vector3.new(0, 0, 0), "rbxassetid://3370684035", 0, Color3.new(255, 0, 0), "rbxassetid://429400881", 1, 1)
                rs.Damage:FireServer(v.Humanoid, v.Head.CFrame, 190, 0, Vector3.new(0, 0, 0), "rbxassetid://3370684035", 0, Color3.new(255, 0, 0), "rbxassetid://429400881", 1, 1)
                for _, offset in ipairs(offsets) do
                rs.Damage12:FireServer(hum, v.HumanoidRootPart.CFrame * CFrame.new(offset[1], offset[2], -0.5), 0, 0, Vector3.new(0, 0, 0), 0.004, "", 1, 1)
                end
            end
        end
    end
    wait(4)
    CDC = false
end

------------------------------------------Dash-------------------------------------------

function Dashyat2()
plr.Character.Humanoid.WalkSpeed = 80
plr.Character.Humanoid.JumpPower = 100
   for i = 1,10 do
   rs.Dodge:FireServer()
   wait()
   rs.VampireDash:FireServer(BrickColor.new("Crimson"))
   end
   plr.Character.Humanoid.WalkSpeed = 20
   plr.Character.Humanoid.JumpPower = 50
end

---------------------------------Pose----------------------------------------------

local posing = false
function jojopose()
if posing == false then
posing = true
pose = Instance.new("Animation")
pose.AnimationId = assettest(4659036460)
poseanim = plr.Character.Humanoid:LoadAnimation(pose)
poseanim:Play(0.1,1,1)
poseanim:AdjustSpeed(1)
wait(2.45)
rs.Menacing:FireServer(true)
poseanim:AdjustSpeed(0)
plr.Character.Humanoid.WalkSpeed = 0
plr.Character.Humanoid.JumpPower = 0
repeat 
rs.Damage12:FireServer(game.Players.LocalPlayer.Character.Humanoid, game.Players.LocalPlayer.Character["Left Arm"].CFrame, 0, 0, Vector3.new(0, 0, 0), 0.01, "", 1, 1)
rs.Damage12:FireServer(hum, hrp.CFrame * CFrame.new(20, 0, 8), 0, 0, Vector3.new(0, 0, 0), 0.05, "", 1, 1)
rs.Damage12:FireServer(hum, hrp.CFrame * CFrame.new(20, 5, 8), 0, 0, Vector3.new(0, 0, 0), 0.05, "", 1, 1)
rs.Damage12:FireServer(hum, hrp.CFrame * CFrame.new(20, 10, 8), 0, 0, Vector3.new(0, 0, 0), 0.05, "", 1, 1)
rs.Damage12:FireServer(hum, hrp.CFrame * CFrame.new(18, 15, 8), 0, 0, Vector3.new(0, 0, 0), 0.05, "", 1, 1)
rs.Damage12:FireServer(hum, hrp.CFrame * CFrame.new(16, 20, 8), 0, 0, Vector3.new(0, 0, 0), 0.05, "", 1, 1)
rs.Damage12:FireServer(hum, hrp.CFrame * CFrame.new(14, 25, 8), 0, 0, Vector3.new(0, 0, 0), 0.05, "", 1, 1)
rs.Damage12:FireServer(hum, hrp.CFrame * CFrame.new(10, 28, 8), 0, 0, Vector3.new(0, 0, 0), 0.05, "", 1, 1)
rs.Damage12:FireServer(hum, hrp.CFrame * CFrame.new(5, 30, 8), 0, 0, Vector3.new(0, 0, 0), 0.05, "", 1, 1)
rs.Damage12:FireServer(hum, hrp.CFrame * CFrame.new(-20, 0, -8), 0, 0, Vector3.new(0, 0, 0), 0.05, "", 1, 1)
rs.Damage12:FireServer(hum, hrp.CFrame * CFrame.new(-20, 5, -8), 0, 0, Vector3.new(0, 0, 0), 0.05, "", 1, 1)
rs.Damage12:FireServer(hum, hrp.CFrame * CFrame.new(-20, 10, -8), 0, 0, Vector3.new(0, 0, 0), 0.05, "", 1, 1)
rs.Damage12:FireServer(hum, hrp.CFrame * CFrame.new(-18, 15, -8), 0, 0, Vector3.new(0, 0, 0), 0.05, "", 1, 1)
rs.Damage12:FireServer(hum, hrp.CFrame * CFrame.new(-16, 20, -8), 0, 0, Vector3.new(0, 0, 0), 0.05, "", 1, 1)
rs.Damage12:FireServer(hum, hrp.CFrame * CFrame.new(-14, 25, -8), 0, 0, Vector3.new(0, 0, 0), 0.05, "", 1, 1)
rs.Damage12:FireServer(hum, hrp.CFrame * CFrame.new(-10, 28, -8), 0, 0, Vector3.new(0, 0, 0), 0.05, "", 1, 1)
rs.Damage12:FireServer(hum, hrp.CFrame * CFrame.new(-5, 30, -8), 0, 0, Vector3.new(0, 0, 0), 0.05, "", 1, 1)
wait(0.5)
until posing == false
else
rs.Menacing:FireServer(false)
plr.Character.Humanoid.WalkSpeed = 18
plr.Character.Humanoid.JumpPower = 50
posing = false
poseanim:Stop()
end
end

--------------------------------------------------------------------------------------


mouse.KeyDown:Connect(function(key)
if key == "e" then
        if death == true then return end
        cleave()
    end
if key == "f" then
        if death == true then return end
        Expansion()
    end
if key == "c" then
        if death == true then return end
        Dashyat2()
    end
if key == "p" then
        if death == true then return end
        jojopose()
    end
end)



