function Whiny()
    local pjspeed = 90
    local mod = Instance.new("Model")
    mod.Parent = chr
    local hitbox1 = Instance.new("Part")
    local hitbox2 = hitbox1:Clone()
    hitbox2.CFrame = chr.HumanoidRootPart.CFrame * CFrame.new(0, 0, -29.5)
    hitbox2.Name = "pjhitbox"
    hitbox2.Anchored = false
    hitbox2.CanCollide = false
    hitbox2.Size = Vector3.new(5,5,5)
    hitbox2.Massless = true
    hitbox2.Transparency = 1
    hitbox2.Color = Color3.new(0,0,0)
    hitbox2.Parent = mod
    local projectileboost = Instance.new("BodyVelocity")
    projectileboost.MaxForce = Vector3.new(math.huge,math.huge,math.huge)
    projectileboost.P = math.huge
    projectileboost.Velocity =  hrp.CFrame.LookVector*pjspeed
    projectileboost.Parent = hitbox2
    projectileboost.Name = "chad"
    coroutine.resume(coroutine.create(function()
        game.Debris:AddItem(hitbox2,10)
        wait(10)
        hitbox1:Destroy()
        mod.Parent = game.Workspace
    end))
    coroutine.resume(coroutine.create(function()
        local pjfx = 0
        repeat
            rser.Heartbeat:wait()
            pjfx = pjfx +1
            for i,v in pairs(game.Workspace.Entities:GetChildren()) do
            if v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Name ~= game.Players.LocalPlayer.Name then
                if (hitbox2.Position - v.HumanoidRootPart.Position).Magnitude < 30 then
                    rs.HamonSword:FireServer(v.Humanoid, 1e-320, math.huge, Vector3.new(0, -100000000, 0))
                    end
                end
            end
            rs.ULFDamage:FireServer(hum,hitbox2.CFrame, 0, 0, Vector3.new(0, 0, 0), math.random(0.5, 2), "", 1, 50)
            rs.ULFDamage:FireServer(hum,hitbox2.CFrame, 0, 0, Vector3.new(0, 0, 0), math.random(0.5, 2), "", 1, 50)
            rs.VampireRush:FireServer(game.Players.LocalPlayer.Character.Humanoid, hitbox2.CFrame, 1e-34, 0, Vector3.new(0, 0, 0), "rbxassetid://241837157", 20, Color3.new(255, 255, 255), "rbxassetid://260430079", 0, 0)
        until mod.Parent ~= chr
    end))
end

